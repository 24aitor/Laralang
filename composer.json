    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "v5.4.*"
    },
    "type": "translations",
    "license": "MIT",
    "authors": [
        {
            "name": "Aitor Riba",
            "email": "contact@aitorriba.com",
			"homepage": "https://aitorriba.com/",
			"role": "author"
        },
		{
			"name": "Erik Campobadal",
            "email": "soc@erik.cat",
			"homepage": "https://erik.cat/",
			"role": "developer"
		}
    ],
	"autoload": {
		"psr-4": {
			"Aitor24\\Laralang\\": "./src"
		}
	},
    "extra": {
        "laravel": {
            "providers": [
                "Aitor24\\Laralang\\LaralangServiceProvider"
